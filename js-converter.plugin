#!name=脚本转换器
#!desc=适用于QX，Surge和Loon的脚本转换器
[Script]
http-response ^https?:\/\/(raw|gist)\.githubusercontent.com/.*\.js$ script-path=https://raw.githubusercontent.com/Peng-YM/ScriptConverter/master/js-converter.js, requires-body=true, timeout=10, enabled=false, tag=js-converter.js
[MITM]
hostname = raw.githubusercontent.com,gist.githubusercontent.com
